 &lt;parameter_queries&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;NUM,,BCD,4&amp;quot;" VariableName="v_cuenta_periodos" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select count(*) num&amp;#xA;from  STVTERM&amp;#xA;where STVTERM_CODE = :lb_periodo.clave&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;AVISO,,WideString,4&amp;quot;" VariableName="valida_num_periodos" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select 'show' as aviso&amp;#xA;from  dual&amp;#xA;where :v_cuenta_periodos.NUM &amp;amp;gt; 5&amp;#xA;and   :ed_rut is null&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;PIDM,,Integer,0&amp;quot;" VariableName="v_pidm" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select spbpers_pidm as pidm&amp;#xA;from  spbpers&amp;#xA;where spbpers_name_suffix = :ed_rut&amp;#xA;and   rownum = 1&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;alumno,,WideString,362&amp;quot;" VariableName="v_nombre" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select spriden_first_name||' '||spriden_mi||' '||replace(spriden_last_name,'*',' ') as &amp;quot;alumno&amp;quot;&amp;#xA;from  spriden&amp;#xA;where spriden_pidm = :v_pidm.PIDM&amp;#xA;and   spriden_change_ind is null&amp;#xA;and   :ed_rut is not null&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;NOMBRE,,WideString,20&amp;quot;" VariableName="servidor" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select gwvserv_desc_instancia nombre&amp;#xA;from  gwvserv&amp;#xA;where gwvserv_server = :$Server.Name&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;CONT,,BCD,4&amp;quot;" VariableName="v_cuenta_registros" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select case when count(*) = 0 then 1&amp;#xA;       else null&amp;#xA;       end  as cont&amp;#xA;from   shrtgpa a1,&amp;#xA;       sgbstdn d1,&amp;#xA;       spriden,&amp;#xA;       spbpers&amp;#xA;where  shrtgpa_gpa_type_ind = 'I'&amp;#xA;and    sgbstdn_pidm = shrtgpa_pidm&amp;#xA;and    d1.sgbstdn_term_code_eff = (select max(d2.sgbstdn_term_code_eff)&amp;#xA;                                   from  sgbstdn d2&amp;#xA;                                   where d2.sgbstdn_pidm = shrtgpa_pidm&amp;#xA;                                   and   d2.sgbstdn_term_code_eff &amp;amp;lt;= shrtgpa_term_code&amp;#xA;                                   and   d2.sgbstdn_levl_code = shrtgpa_levl_code)&amp;#xA;and    spriden_pidm = shrtgpa_pidm&amp;#xA;and    spriden_change_ind is null&amp;#xA;and    spbpers_pidm = shrtgpa_pidm&amp;#xA;and   (:ed_rut is null or shrtgpa_pidm = :v_pidm.PIDM)&amp;#xA;and   (null = :lb_periodo.clave or shrtgpa_term_code = :lb_periodo.clave)&amp;#xA;and   (null = :lb_nivel.clave or shrtgpa_levl_code = :lb_nivel.clave)&amp;#xA;and   (null = :lb_carrera.clave or d1.sgbstdn_majr_code_1 = :lb_carrera.clave)&amp;#xA;and   (null = :lb_programa.clave or d1.sgbstdn_program_1 = :lb_programa.clave)&amp;#xA;and    :v_cuenta_periodos.NUM &amp;amp;lt;= 5&amp;#xA;and    :v_cuenta_carreras.NUM &amp;amp;lt;= 5&amp;#xA;and    :Bt_consulta is not null&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
  &lt;parameter_query Columns="&amp;quot;AVISO,,WideString,4&amp;quot;" VariableName="valida_mum_carreras" Sql="&amp;lt;Data&amp;gt;&amp;lt;Lines&amp;gt;select 'show' as aviso&amp;#xA;from  dual&amp;#xA;where :v_cuenta_carreras.NUM &amp;amp;gt; 5&amp;#xA;and   :ed_rut is null&amp;lt;/Lines&amp;gt;&amp;lt;Query&amp;gt;&amp;lt;ADOOptions/&amp;gt;&amp;lt;/Query&amp;gt;&amp;lt;/Data&amp;gt;"/&gt;&#xD;
